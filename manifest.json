{
    "name": "Button's SEO Toolbox",
    "description": "Set of experimental tools to speedup SEO daily routine",
    "version": "0.1",
    "permissions": [
        "storage",
        "tabs"
    ],
    "host_permissions": ["https://*.youtube.com/*"],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [{
        "matches": ["https://app.ahrefs.com/keywords-explorer/google/us/ideas/matchingTerms/*"],
        "js": ["contentScript.js"]
    }],
    "web_accessible_resources": [{
        "resources": [
            "assets/bookmark.png",
            "assets/play.png",
            "assets/delete.png",
            "assets/save.png"
        ],
        "matches": ["https://*.youtube.com/*"]
    }],
    "action": {
        "default_icon": {
            "16": "assets/ext-icon.png",
            "24": "assets/ext-icon.png",
            "32": "assets/ext-icon.png"
        },
        "default_title": "Set of experimental tools to speedup SEO daily routine",
        "default_popup": "popup.html"
    },
    "manifest_version": 3,
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+B",
                "mac": "Command+B"
            }
        }
    }
}